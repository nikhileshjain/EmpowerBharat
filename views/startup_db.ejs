            <div class="text-center">
              <h3>
                ABOUT
              </h3>
              <hr class="my-4">
              <p><%= user.companyName%> is a members-only credit card management and bill payments platform that rewards users every time they pay their credit card bills.</p>
              
            </div>
<br><br>
            <div class="text-center">
                <h3>
                  FOUNDER
                </h3>
                <hr class="my-4">
                <p><strong><%= user.name%></strong></p>
                
                
              </div>
              <br>
              <br>
              <div class="text-center">
                <h3>
                  LEGAL NAME
                </h3>
                <hr class="my-4">
                
                <p><strong>Dreamplug Technologies Private Limited</strong></p>
                
              </div>
          </div>
        </div>
      </div>
      <div class="col-xl-8 order-xl-1">
        <div class="card bg-secondary shadow">
          <div class="card-header bg-white border-0">
            <div class="row align-items-center">
              <div class="col-8">
                <h3 class="mb-0">My account</h3>
              </div>
              <% } %>
              <div class="col-4 text-right">
                <a href="#about" class="btn btn-sm btn-primary">View Statistics</a>
                <a href="#!" class="btn btn-sm btn-primary">Settings</a>

              </div>
            </div>
          </div>
          <div class="card-body">
            <%if(locals.user){%>
            <form method="POST" action="/startup/edit?id=<%=user._id%>" >
              <h6 class="heading-small text-muted mb-4">User information</h6>
              <div class="pl-lg-4">
                <div class="row">
                  <div class="col-lg-6">
                    <div class="form-group focused">
                      <label class="form-control-label" for="input-username">Username</label>
                      <input type="text" id="input-username" class="form-control form-control-alternative" name="name" placeholder="Username" value= <%= user.companyName%>>
                    </div>
                  </div>
                  <div class="col-lg-6">
                    <div class="form-group">
                      <label class="form-control-label" for="input-email">Email address</label>
                      <input name="email" type="email" id="input-email" class="form-control form-control-alternative" placeholder="Email" value= <%= user.email%> >
                    </div>
                  </div>
                  <div class="col-lg-6">
                    <div class="form-group">
                      <label class="form-control-label" for="input-email">Category</label>
                      <input name="category" type="text" class="form-control form-control-alternative" placeholder="Category" value= <%= user.Category%> >
                    </div>
                  </div>
                  <div class="col-lg-6">
                    <div class="form-group">
                      <label class="form-control-label" for="input-email">Product</label>
                      <input name="product" type="text"  class="form-control form-control-alternative" placeholder="Link" value= <%= user.Product%> >
                    </div>
                  </div>
                  <div class="col-lg-6">
                    <div class="form-group">
                      <label class="form-control-label" for="input-email">Logo </label>
                      <input name="logo" type="text"  class="form-control form-control-alternative" placeholder="Link" value= <%= user.Logo%> >
                    </div>
                  </div>

                  <div class="col-lg-6">
                    <div class="form-group">
                      <label class="form-control-label" for="input-email">Website</label>
                      <input name="website" type="text"  class="form-control form-control-alternative" placeholder="Link" value= <%= user.Website%> >
                    </div>
                  </div>
                </div>
                
                
              </div>
              <hr class="my-4">
              <!-- Address -->
              <h6 class="heading-small text-muted mb-4">Contact information</h6>
              <div class="pl-lg-4">
                <div class="row">
                  <div class="col-md-12">
                    <div class="form-group focused">
                      <label class="form-control-label" name="address" for="input-address">Address</label>
                      <!-- Bld Mihail Kogalniceanu, nr. 8 Bl 1, Sc 1, Ap 09 -->
                      <input name="address" id="input-address" class="form-control form-control-alternative" placeholder="Home Address" value="<%= user.Address%>" type="text">
                    </div>
                  </div>
                </div>
                <div class="row">
                  <div class="col-lg-4">
                    <div class="form-group focused">
                      <label class="form-control-label" name="city" for="input-city">City</label>
                      <input name="city" type="text" id="input-city" class="form-control form-control-alternative" placeholder="City" value="<%= user.City%>">
                    </div>
                  </div>
                  <div class="col-lg-4">
                    <div class="form-group focused">
                      <label class="form-control-label" for="input-country">Country</label>
                      <input name="country" type="text" id="input-country" class="form-control form-control-alternative" placeholder="Country" value="<%= user.Country%>">
                    </div>
                  </div>
                  <div class="col-lg-4">
                    <div class="form-group">
                      <label class="form-control-label" for="input-country">Postal code</label>
                      <input name="code" type="number" id="input-postal-code" class="form-control form-control-alternative" placeholder="Postal Code" value=<%= user.Country%>
                    </div>
                  </div>
                </div>
              </div>
              <!-- <hr class="my-4"> -->
              <!-- Description -->
              <input type="submit"  class="btn btn-sm btn-primary" value="Save Changes" />
            </form>
            <% } %>
          </div>
