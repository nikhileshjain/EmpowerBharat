            <form method="POST" action="/investor/edit?id=<%=user._id%>"  >
              <h6 class="heading-small text-muted mb-4">User information</h6>
              <div class="pl-lg-4">
                <div class="row">
                  <div class="col-lg-6">
                    <div class="form-group focused">
                      <label class="form-control-label" for="input-username">Username</label>
                      <input type="text" id="input-username" class="form-control form-control-alternative" name="name" placeholder="Username" value= <%= user.name%>>
                    </div>
                  </div>
                  <div class="col-lg-6">
                    <div class="form-group">
                      <label class="form-control-label" for="input-email">Email address</label>
                      <input type="email" id="input-email" class="form-control form-control-alternative" name="email"  placeholder= <%= user.email %> value= <%= user.email %>>
                    </div>
                  </div>
                  <div class="col-lg-6">
                    <div class="form-group focused">
                      <label class="form-control-label" for="input-username">Image Link</label>
                      <input type="text" id="input-username" class="form-control form-control-alternative" name="image" placeholder="Profile Image Link" value= <%= user.image%>>
                    </div>
                  </div>

                </div>
              
                
              </div>
